{
  "name": "parsley-language",
  "displayName": "Parsley Language Support",
  "description": "Syntax highlighting and language support for the Parsley programming language",
  "version": "0.18.0",
  "publisher": "sambeau",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "parsley",
    "basil",
    "web",
    "jsx",
    "html",
    "templating"
  ],
  "contributes": {
    "languages": [
      {
        "id": "parsley",
        "aliases": [
          "Parsley",
          "parsley"
        ],
        "extensions": [
          ".pars",
          ".part"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "parsley",
        "scopeName": "source.parsley",
        "path": "./syntaxes/parsley.tmLanguage.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sambeau/basil.git"
  },
  "license": "MIT"
}
